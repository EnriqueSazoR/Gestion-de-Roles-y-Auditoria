generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


// Modelo de prueba para probar conexi√≥n
model user {
  id Int @id
  nombre String @unique
}
